import { Column, Entity, Index, PrimaryKey } from "typeorm";





@Entity("chii_timeline", { schema: "bangumi" })
export class ChiiTimeline {
  @PrimaryKey({ type: "int", name: "tml_id", unsigned: true })
  tmlId: number;

  @Column("mediumint", {
    name: "tml_uid",
    unsigned: true,
    default: "'0'",
  })
  tmlUid: number;

  @Column("smallint", { name: "tml_cat", unsigned: true })
  tmlCat: number;

  @Column("smallint", {
    name: "tml_type",
    unsigned: true,
    default: "'0'",
  })
  tmlType: number;

  @Column("char", { name: "tml_related", length: 255, default: "'0'" })
  tmlRelated: string;

  @Column("mediumtext", { name: "tml_memo" })
  tmlMemo: string;

  @Column("mediumtext", { name: "tml_img" })
  tmlImg: string;

  @Column("tinyint", { name: "tml_batch", unsigned: true })
  tmlBatch: number;

  @Column("tinyint", {
    name: "tml_source",
    comment: "更新来源",
    unsigned: true,
    default: "'0'",
  })
  tmlSource: number;

  @Column("mediumint", {
    name: "tml_replies",
    comment: "回复数",
    unsigned: true,
  })
  tmlReplies: number;

  @Column("int", { name: "tml_dateline", unsigned: true, default: "'0'" })
  tmlDateline: number;

  @Column("tinyint", {
    name: "tml_status",
    unsigned: true,
    default: "'0'",
  })
  tmlStatus: number;
}
