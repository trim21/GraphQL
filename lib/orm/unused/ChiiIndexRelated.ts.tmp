import { Column, Entity, Index, PrimaryKey } from "typeorm";






@Entity("chii_index_related", { schema: "bangumi" })
export class ChiiIndexRelated {
  @PrimaryKey({
    type: "mediumint",
    name: "idx_rlt_id",
    unsigned: true,
  })
  idxRltId: number;

  @Column("tinyint", { name: "idx_rlt_cat" })
  idxRltCat: number;

  @Column("mediumint", {
    name: "idx_rlt_rid",
    comment: "关联目录",
    unsigned: true,
  })
  idxRltRid: number;

  @Column("smallint", {
    name: "idx_rlt_type",
    comment: "关联条目类型",
    unsigned: true,
  })
  idxRltType: number;

  @Column("mediumint", {
    name: "idx_rlt_sid",
    comment: "关联条目ID",
    unsigned: true,
  })
  idxRltSid: number;

  @Column("mediumint", {
    name: "idx_rlt_order",
    unsigned: true,
    default: "'0'",
  })
  idxRltOrder: number;

  @Column("mediumtext", { name: "idx_rlt_comment" })
  idxRltComment: string;

  @Column("int", { name: "idx_rlt_dateline", unsigned: true })
  idxRltDateline: number;
}
