import { Column, Entity, Index, PrimaryKey } from "typeorm";


@Entity("chii_index_collects", { schema: "bangumi" })
export class ChiiIndexCollects {
  @PrimaryKey({
    type: "mediumint",
    name: "idx_clt_id",
    unsigned: true,
  })
  idxCltId: number;

  @Column("mediumint", {
    name: "idx_clt_mid",
    comment: "目录ID",
    unsigned: true,
  })
  idxCltMid: number;

  @Column("mediumint", {
    name: "idx_clt_uid",
    comment: "用户UID",
    unsigned: true,
  })
  idxCltUid: number;

  @Column("int", { name: "idx_clt_dateline", unsigned: true })
  idxCltDateline: number;
}
